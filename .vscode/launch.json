            "args": [
                "20250217_smon.txt"
            ]
        },
        {
            "name": "SMON Old Version (w/ args)",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}",
